<Window x:Class="SimplexViewer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimplexViewer"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="ObjectiveFunctionEntry" HorizontalAlignment="Left" Height="23" Margin="46,61,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="ConstraintEntry" HorizontalAlignment="Left" Height="78" Margin="46,95,0,0" AcceptsReturn="True" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="SubmitButton" Content="Solve" HorizontalAlignment="Left" Margin="46,178,0,0" VerticalAlignment="Top" Width="75" Click="SubmitButtonClick"/>
        <Label x:Name="ResultsLabel" HorizontalAlignment="Left" Margin="46,224,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
